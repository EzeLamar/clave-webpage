{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "enabled": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "detailSlug": {
      "type": "string"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.products",
        "blocks.markdown",
        "blocks.hero",
        "blocks.about-us"
      ],
      "required": true
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    }
  }
}
